{
  "name": "stencila-jupyterer",
  "version": "0.0.1",
  "description": "Stencila compiler for Jupyter",
  "main": "lib/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "lint": "standard \"lib/**/*.js\" \"bin/**/*.js\" \"test/**/*.js\"",
    "check": "dependency-check ./package.json && dependency-check ./package.json --unused --no-dev --ignore-module request",
    "test": "tape test | tap-spec",
    "cover": "nyc --reporter=lcov --reporter=text npm test",
    "docs": "esdoc",
    "prepublishOnly": "npm run lint && npm run test && npm run check && npm run docs",
    "clean": "rimraf .nyc_output docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stencila/jupyterer.git"
  },
  "keywords": [],
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/stencila/jupyterer/issues"
  },
  "homepage": "https://github.com/stencila/jupyterer#readme",
  "devDependencies": {
    "dependency-check": "^3.1.0",
    "esdoc": "^1.0.4",
    "esdoc-node": "^1.0.3",
    "esdoc-standard-plugin": "^1.0.0",
    "node-mocks-http": "^1.6.7",
    "nodemon": "^1.17.2",
    "nyc": "^11.6.0",
    "rimraf": "^2.6.2",
    "standard": "^11.0.1",
    "tap-spec": "^4.1.1",
    "tape": "^4.9.0"
  },
  "dependencies": {
    "jmp": "^1.0.0",
    "kernelspecs": "^2.0.0",
    "request": "^2.85.0",
    "spawnteract": "^4.0.0",
    "uuid": "^3.2.1"
  },
  "esdoc": {
    "source": "./lib",
    "destination": "./docs",
    "plugins": [
      {
        "name": "esdoc-standard-plugin"
      },
      {
        "name": "esdoc-node"
      }
    ]
  },
  "standard": {
    "ignore": []
  }
}
